Base pan output test from the /
contentspath is not relevant, uses CCM.contents anyway
---
renderpath=/
rendermodule=pan
contentspath=/
element=truefalse,doublequote
---
^"/data/hash/x" = "OK"; # string$
^"/data/listtruefalse/1" = true; # boolean$
^"/data/listtruefalse/2" = false; # boolean$
^"/data/one" = 1; # long$
^"/data/oneandahalf" = 1.5; # double$
^"/special/escaped_20data" = "escaped data"; # string$
^"/special/not_escaped_d" = "not_escaped_d"; # string$
^"/z/deep/a" = "a"; # string$
^"/z/deep/list/1/a/b" = "a"; # string$
^"/z/deep/list/2/atest" = 1; # long$
^"/z/deep/list/2/btest" = 1.5; # double$
^"/z/deep/list/2/ctest" = true; # boolean$
^"/z/deep/list/2/dtest" = false; # boolean$
^"/z/deep/list/2/etest" = "ok"; # string$
^"/z/deep/list/3/fake" = 1; # long$
